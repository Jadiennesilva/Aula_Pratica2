#include<stdio.h>
#include<stdlib.h>
#include <locale.h>

int MA (int NU1, int NU2){
	
	setlocale(LC_ALL, "portuguese_Brazil");
	
	int MA;
	if (NU1 > NU2){
	MA = NU1;
		
	
	}else{
	MA = NU2;
	}
	
	return MA;
}

int main(){
	int M , NOTA1 , NOTA2;
	printf("Digite o primeiro numero : \n");
	scanf("%d", &NOTA1);
	printf("Digite o segundo numero : \n");
	scanf("%d", &NOTA2);
	
	M = MA (NOTA1,NOTA2);
	printf(" O maior numero  E : %d",M);

	
	
	
	return 0;
}
