#include<stdio.h>
#include<stdlib.h>
#include <locale.h>

int D(int N1, int N2){
	
	setlocale(LC_ALL, "portuguese_Brazil");
	
	int MA, DA;
	if (N1 > N2){
	MA = N1;
		
	
	}else{
	MA = N2;
	}
	DA = 2 * MA;
	
	return DA;
}

int main(){
	int  NOTA1 , NOTA2, D;
	printf("Digite o primeiro numero : \n");
	scanf("%d", &NOTA1);
	printf("Digite o segundo numero : \n");
	scanf("%d", &NOTA2);
	
	DA = D (NOTA1,NOTA2);
	printf(" O maior numero  E : %d",DA);

	
	
	
	return 0;
}
