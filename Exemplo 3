#include <stdio.h>
#include <stdlib.h>

int dobro (int Num){
	int d;
	d = 2 * Num;
	
return d;					
} 

int main(){
	int n, x;
    printf("digite um numero:");
    scanf("%d",&n);
    
    printf("digite o segundo numero:");
    scanf("%d",&n);
    
    x = dobro(n);
    if(n > x){
    	
    printf("%d",x);
    	
	}else{
	printf("");
	
	printf("");
		
	}
    
  
   
   
   

return 0;
}

